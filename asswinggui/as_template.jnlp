<?xml version="1.0" encoding="UTF-8"?>
<jnlp codebase="http://www.geopublishing.org/gp-${project.version}/" href="http://www.geopublishing.org/gp-${project.version}/as.jnlp" spec="1.0+">
  
  <information>
    <title>AtlasStyler SLD/SE Editor ${project.version}</title>
    <description>A graphical editor to create OGC SLD/SE files</description>
    <homepage href="http://en.geopublishing.org/AtlasStyler"/>    
    <vendor>en.geopublishing.org</vendor>
    <icon kind="default" href="icon64_AS.gif" height="64" width="64"/> 
    <icon kind="splash" href="startAS_en.gif"/>    
    
	<shortcut online="true">
	  <desktop/>
	  <menu submenu="Geopublishing"/>
	</shortcut>    
    
    <offline-allowed/>
  </information>
  
  <information locale="de">
    <title>AtlasStyler SLD/SE Editor ${project.version}</title>
    <description>Grafischer Editor f√ºr OGC SLD/SE Dateien</description>
    <homepage href="http://de.geopublishing.org/AtlasStyler"/>    
    <vendor>de.geopublishing.org</vendor>
    <icon kind="default" href="icon64_AS.gif" height="64" width="64"/> 
    <icon kind="splash" href="startAS_de.gif"/>    
    
	<shortcut online="true">
	  <desktop/>
	  <menu submenu="Geopublishing"/>
	</shortcut>    
    
    <offline-allowed/>
  </information>
  
  <information locale="fr">
    <title>AtlasStyler SLD/SE Editor ${project.version}</title>
    <description>A graphical editor to create OGC SLD/SE files</description>
    <homepage href="http://fr.geopublishing.org/AtlasStyler"/>    
    <vendor>fr.geopublishing.org</vendor>
    <icon kind="default" href="icon64_AS.gif" height="64" width="64"/> 
