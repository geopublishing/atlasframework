  <security>
    <all-permissions/>
  </security>
  
  <update check="always" policy="always"/>
  
  <resources>
    <java href="http://java.sun.com/products/autodl/j2se" java-vm-args="-Dfile.encoding=UTF-8" max-heap-size="320m" version="1.6.0_14+"/>

    <property name="jnlp.packEnabled" value="true"/>
	<property name="file.encoding" value="UTF-8"/>
	
	<extension href="http://www.wikisquare.de/swt/swt-3.7M5.jnlp" name="SWT"/>
	<extension href="http://www.wikisquare.de/jai/jai-1.1.3.jnlp" name="JAI"/>
	
	$dependencies
		
  </resources>
  
  <application-desc main-class="$mainClass"/>
</jnlp>
